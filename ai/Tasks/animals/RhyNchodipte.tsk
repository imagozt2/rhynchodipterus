  <BFAITaskTemplate Name="InviteReproduction" TaskDelayMin="60" TaskDelayMax="180" UniqueID="RhyNchodipte:InviteReproduction">
    <BFAICreateData>
      <Subjects>
        <RhyNchodipte_Adult_F/>
        <Qualifiers_AND isSwimming="true" b_Pregnant="E false"/>
      </Subjects>
      <Targets>
